<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jason.tcpdemo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/time"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".funcs.FuncTcpClient"/>
        <activity android:name=".funcs.FuncTcpClient_2"/>
        <activity android:name=".funcs.FuncTcpServer"/>
        <activity android:name=".funcs.FuncTcpServer_2"/>
        <activity android:name=".funcs.FuncTcpServer_2_beep"/>
        <activity android:name=".funcs.FuncTcpServer_3"/>
    </application>

</manifest>